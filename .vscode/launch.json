    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) CMake Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "cwd": "${command:cmake.launchTargetDirectory}",
            "args": []
        },
        {
            "name": "(lldb) Attach",
            "type": "lldb",
            "request": "attach",
            "program": "${command:cmake.launchTargetPath}"
        }

    ]
}